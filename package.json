{
  "name": "reactive-shared",
  "version": "1.1.0",
  "description": "set of libraries for build apps based on lists and forms",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julian-reactive/reactive-shared.git"
  },
  "author": "julian mosquera",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julian-reactive/reactive-shared/issues"
  },
  "homepage": "https://github.com/julian-reactive/reactive-shared#readme",
  "ts-standard": {
    "project": "./tsconfig.json",
    "ignore": [
      "node_modules",
      ".history",
      "dist"
    ],
    "plugins": [
      "react-hooks"
    ]
  },
  "peerDependencies": {
    "@date-io/date-fns": "^3.0.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/base": "^5.0.0-beta.58",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/system": "^6.1.6",
    "@mui/x-date-pickers": "^7.22.2",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.28.0",
    "yup": "^1.4.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-devtools": "^5.59.16",
    "axios": "^0.27.2",
    "chalk": "^5.3.0",
    "pluralize": "^8.0.0",
    "react-hook-form": "^7.53.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-number-format": "^5.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.0.0",
    "html-webpack-plugin": "^5.6.3",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^4.0.0",
    "terser": "^5.43.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.100.2",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production && tsc --emitDeclarationOnly",
    "build:dev": "webpack --mode development",
    "analyze": "webpack --mode production --env analyze",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run type-check"
  }
}
